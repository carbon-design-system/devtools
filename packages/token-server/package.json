{
  "name": "@carbon/devtools-token-server",
  "version": "2.4.0",
  "private": true,
  "description": "A distribution server that maintains and versions Carbon tokens separately.",
  "main": "app.js",
  "scripts": {
    "build": "webpack --config webpack.js",
    "watch": "webpack --watch --config webpack.js",
    "start": "yarn nodemon index.js",
    "deploy": "yarn build && ibmcloud cf push",
    "clean": "rimraf public && rimraf node_modules",
    "format": "prettier --write '*.{js,json,md,scss,ts}' '**/*.{js,json,md,scss,ts}' '!**/{dist,build,es,lib,storybook,ts,umd}/**' '!**/*.{jpg,jpeg,gif,png}'",
    "format:diff": "prettier --list-different '*.{js,json,md,scss,ts}' '**/*.{js,json,md,scss,ts}' '!**/{dist,build,es,lib,storybook,ts,umd}/**' '!**/*.{jpg,jpeg,gif,png}'",
    "lint": "yarn lint:js",
    "lint:js": "eslint . --ext .js",
    "test": "yarn format && yarn lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/devtools"
  },
  "keywords": [
    "IBM",
    "Carbon",
    "Devtools",
    "Tools",
    "Development",
    "Tokens"
  ],
  "license": "Apache-2.0",
  "author": "James Dow",
  "bugs": {
    "url": "https://github.com/carbon-design-system/devtools/issues/new",
    "email": "james.dow@us.ibm.com"
  },
  "homepage": "https://github.com/carbon-design-system/devtools#readme",
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@carbon/colors": "^10.19.0",
    "@carbon/ibm-security": "^1.38.0",
    "@carbon/ibmdotcom-react": "^1.15.0",
    "@carbon/ibmdotcom-utilities": "^1.15.0",
    "@carbon/layout": "^10.17.0",
    "@carbon/themes": "^10.26.0",
    "@carbon/type": "^10.20.0",
    "babel-loader": "^8.2.2",
    "carbon-components-react": "^7.27.0",
    "clean-webpack-plugin": "^3.0.0",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  },
  "devDependencies": {
    "eslint": "^7.19.0",
    "eslint-config-carbon": "^2.1.0",
    "prettier": "^2.2.1",
    "prettier-config-carbon": "^0.4.0",
    "rimraf": "^3.0.2"
  },
  "eslintConfig": {
    "extends": [
      "carbon"
    ],
    "globals": {
      "chrome": "readonly"
    }
  },
  "sideEffects": false,
  "engines": {
    "node": "14.x"
  }
}
