{
  "name": "carbon-devtools",
  "version": "2.4.0",
  "private": true,
  "description": "Basic tools to aide in the development of Carbon pages.",
  "main": "dist/manifest.json",
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "watch": "webpack --watch --config webpack.dev.js",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "clean": "rimraf dist && rimraf node_modules",
    "format": "prettier --write '*.{js,json,md,scss,ts}' '**/*.{js,json,md,scss,ts}' '!**/{dist,build,es,lib,storybook,ts,umd}/**' '!**/*.{jpg,jpeg,gif,png}'",
    "format:diff": "prettier --list-different '*.{js,json,md,scss,ts}' '**/*.{js,json,md,scss,ts}' '!**/{dist,build,es,lib,storybook,ts,umd}/**' '!**/*.{jpg,jpeg,gif,png}'",
    "lint": "yarn lint:js && yarn lint:css",
    "lint:js": "eslint . --ext .js",
    "lint:css": "stylelint 'src/**/*.{css,scss}' --report-needless-disables --report-invalid-scope-disables",
    "test": "yarn format && yarn lint"
  },
  "dependencies": {
    "@carbon/colors": "^10.19.0",
    "@carbon/grid": "^10.19.0",
    "@carbon/ibm-security": "^1.38.0",
    "@carbon/ibmdotcom-react": "^1.15.0",
    "@carbon/ibmdotcom-utilities": "^1.15.0",
    "@carbon/icons": "^10.24.0",
    "@carbon/icons-react": "^10.24.0",
    "@carbon/layout": "^10.17.0",
    "@carbon/motion": "^10.12.0",
    "@carbon/themes": "^10.26.0",
    "@carbon/type": "^10.20.0",
    "carbon-components": "^10.27.0",
    "carbon-components-react": "^7.27.0",
    "color": "^3.1.3",
    "node-fetch": "^2.6.1",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "cssnano": "^4.1.10",
    "dotenv-webpack": "^6.0.0",
    "eslint": "^7.19.0",
    "eslint-config-carbon": "^2.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.1",
    "mini-css-extract-plugin": "^1.3.5",
    "node-sass": "^5.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-import": "^14.0.0",
    "postcss-loader": "^5.0.0",
    "postcss-preset-env": "^6.7.0",
    "postcss-rem-to-pixel": "^4.1.2",
    "preact": "^10.5.12",
    "prettier": "^2.2.1",
    "prettier-config-carbon": "^0.4.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "rimraf": "^3.0.2",
    "sass-loader": "^10.1.1",
    "style-loader": "^2.0.0",
    "stylelint": "^13.9.0",
    "stylelint-config-carbon": "^1.2.0",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.20.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3",
    "zip-webpack-plugin": "^4.0.1"
  },
  "sideEffects": false,
  "keywords": [
    "IBM",
    "Dotcom",
    "IBM.com",
    "Carbon",
    "Tools",
    "Development"
  ],
  "author": "James Dow",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/carbon-design-system/devtools"
  },
  "bugs": {
    "url": "https://github.com/carbon-design-system/devtools/issues/new",
    "email": "james.dow@us.ibm.com"
  },
  "homepage": "https://github.com/carbon-design-system/devtools#readme",
  "eslintConfig": {
    "extends": [
      "carbon"
    ],
    "globals": {
      "chrome": "readonly"
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-carbon"
    ],
    "rules": {
      "max-nesting-depth": 3
    }
  },
  "engines": {
    "node": "14.x"
  }
}
